
@{
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group">
            <label class="control-label mb-1">Chọn Đại Lý</label>
            @Html.DropDownList("DaiLyId", null, htmlAttributes: new { })
        </div>

        <div class="form-group">
            <label class="control-label mb-1">Chọn Ngày Đăng Ký</label>
            <input type="date" name="ngayDangKy" value="@ViewBag.DateNow" />
        </div>

        <div class="form-group">
            <input type="submit" value="Create" class="btnbbbccc btn-default" />
        </div>
    </div>
}

