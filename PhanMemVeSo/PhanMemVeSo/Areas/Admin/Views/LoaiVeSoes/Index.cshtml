@model IEnumerable<Model.EFModels.LoaiVeSo>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<div class="breadcrumbs">
    <div class="col-sm-4">
        <div class="page-header float-left">
            <div class="page-title">
                <h1>Dashboard</h1>
            </div>
        </div>
    </div>
    <div class="col-sm-8">
        <div class="page-header float-right">
            <div class="page-title">
                <ol class="breadcrumb text-right">
                    <li class="active">Quản lý loại vé số</li>
                </ol>
            </div>
        </div>
    </div>
</div>



<div class="content mt-3">
    <div class="animated fadeIn">
        <div class="row">

            <div class="col-md-12">
                <div class="card">
                    <div class="card-header">
                        <strong class="card-title">Loại vé số</strong>
                    </div>
                    <div class="card-body">
                        <table id="bootstrap-data-table" class="table table-striped table-bordered">
                            <thead>
                                <tr>
                                    <th>
                                        @Html.DisplayNameFor(model => model.TenTinh)
                                    </th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.TenTinh)
                                        </td>
                                        <td>
                                            @Html.ActionLink("Sửa", "Edit", new { id = item.LoaiVeSoId }) |
                                            @Html.ActionLink("Thông tin", "Details", new { id = item.LoaiVeSoId }) |
                                            @Html.ActionLink("Xóa", "Delete", new { id = item.LoaiVeSoId })
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                        <p>
                            @Html.ActionLink("Create New", "Create")
                        </p>
                    </div>

                </div>

            </div>


        </div>
    </div><!-- .animated -->
</div>

